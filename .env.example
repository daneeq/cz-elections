# ps2021_fixture | ps2025_live
DATA_MODE=ps2021_fixture
# live endpoints (override if they change)
VOLBY2025_TOPLINE=https://www.volby.cz/appdata/ps2025/odata/vysledky.xml

# Capture Configuration
# ===================

# Logging level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO

# HTTP request settings
CAPTURE_HTTP_TIMEOUT=30        # Request timeout in seconds
CAPTURE_MAX_RETRIES=3          # Maximum retry attempts for failed requests

# Rate limiting (seconds between requests)
CAPTURE_STATEFUL_DELAY=2       # Delay between stateful feed requests
CAPTURE_BATCH_DELAY=1          # Delay between batch requests

# Capture intervals
CAPTURE_STATEFUL_INTERVAL=60   # Stateful capture interval in seconds
CAPTURE_POLL_GRACE=5           # Grace period for polling cycles

# Data directories
DATA_DIR=data                  # Base data directory
S3_BUCKET=                     # S3 bucket for data storage (optional)
S3_PREFIX=                     # S3 key prefix (optional)
AWS_REGION=eu-central-1        # AWS region for S3
